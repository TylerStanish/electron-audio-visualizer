{
  "name": "AudioVisualizer",
  "version": "1.0.0",
  "description": "A minimal audio visualizer electron application",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && electron build/main.js",
    "build": "tsc --outDir build src/*.ts",
    "build-watch": "tsc --outDir build *.ts --watch",
    "clean": "rm -rf build",
    "dev": "npm run clean && npm run build-watch"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Tyler Stanish",
  "dependencies": {
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "electron": "^2.0.8"
  }
}
